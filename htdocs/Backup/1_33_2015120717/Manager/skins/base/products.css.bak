@charset "utf-8";

/*产品属性*/
.attribute_table{ width:100%;}
.attribute_table td{line-height:30px;}
.attribute_table td.attribute_td_title{ width:55px;}
.attribute_dropdownlist select{ width:128px;}
.attribute_dropdownlist .s_selt{ width:120px;}
.attribute_table td .e_radio{ width:60px; margin-right:0;}

/*产品分类列表*/
.nsw_add_proclsify span.f_att_pd_column{background-position:-362px -262px;padding-left:25px; width:55px;}

/*=============添加分类=============*/
.attr_btn {display: inline-block;padding:0 6px;line-height:27px;border:1px solid #dcdcdc;cursor: pointer;}
.create_attr i {float:left;display: inline-block;margin: 5px 5px 0 0;width: 16px;height: 16px;background: url(../default/images/main_sprit1.png) no-repeat -647px -62px;}
.attr_btn:hover {color: #fff;background: #f08300;border-color: #f08300;}
.create_attr:hover i {background: url(../default/images/main_sprit1.png) no-repeat -647px -86px;}
.importing i {float:left;display: inline-block;margin: 5px 5px 0 0;width: 16px;height: 16px;background: url(../default/images/main_sprit1.png) no-repeat -678px -62px;}
.importing:hover i {background: url(../default/images/main_sprit1.png) no-repeat -678px -86px;}

/*分类导入*/
.colu_list {width: 70%;border: 1px solid #dcdcdc;}
.colu_list li {float: left;padding: 4px 0;width: 150px;margin-left:20px;display:inline;line-height: 27px;overflow: hidden;}

/*添加属性*/
.attr_objbox .add_label{ padding-left:0;}
.attr_objbox .add_r{ width:86%;}
.attr_objbox .color_box input{ line-height:24px;}